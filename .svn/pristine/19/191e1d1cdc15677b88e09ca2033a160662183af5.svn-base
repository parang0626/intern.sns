
html, body, aside {
	height : 100%;
}

aside {
	width: 10%;
	min-width : 60px;
	float: left;
	background-color: #ccc;
}

section {
	margin-right: 10px;
	margin-bottom: 10px;
	min-width : 300px;
	width: 80%;
	float: right;
}

ul {
	margin-top: 10px;
	padding-left:0px;
}

li  {
	list-style: none;
	height : 50px;
	color: white;
	line-height: 50px;
	vertical-align: middle;
	text-align: left;
	margin-left : 10px;
	border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
	
}

.sidemenu {
	margin-left : 10px;
}